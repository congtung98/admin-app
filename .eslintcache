tegories) }\n                        </ul>\n                    </Col>\n                </Row>\n            </Container>\n            <Modal \n                show={show}\n                handleClose={handleClose}\n                modalTitle={'Add New Category'}\n            >\n                <Input\n                        value={categoryName}\n                        placeholder={`Category Name`}\n                        onChange={(e) => setCategoryName(e.target.value)}\n                    />\n\n                    <select \n                    className=\"form-control\"\n                    value = {parentCategoryId} \n                    onChange={(e) => setParentCategoryId(e.target.value)}>\n                        <option>Select category</option>\n                        {\n                            createCategoryList(category.categories).map(option =>\n                                <option key={option.value} value={option.value}>{option.name}</option>\n                            )\n                        }\n                    </select>\n\n                    <input type=\"file\" name=\"categoryImage\" onChange={handleCategoryImage}/>\n            </Modal>\n            \n        </Layout>\n    )\n}\n\nexport default Category\n","/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/redux/actions/category.actions.js",[],"/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/redux/reducers/category.reducers.js",["140","141","142","143"],"/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/components/UI/Modal/index.js",[],"/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/redux/actions/initialData.action.js",["144"],"/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/redux/reducers/product.reducers.js",["145","146"],{"ruleId":"147","severity":1,"message":"148","line":24,"column":6,"nodeType":"149","endLine":24,"endColumn":8,"suggestions":"150"},{"ruleId":"1