 = (props) => {\n\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n    const [error, setError] = useState('');\n    const auth = useSelector(state => state.auth);\n\n    const dispatch = useDispatch();\n\n  \n\n    const userLogin = (e) => {\n\n        e.preventDefault();\n\n        const user = {\n            email,\n            password\n        }\n\n        dispatch(login(user));\n    }\n\n    if(auth.authenticate){\n        return <Redirect to={`/`} />\n    }\n    return (\n        <Layout>\n            <Container>\n                <Row style={{ marginTop: '50px' }}>\n                    <Col md={{ span: 6, offset: 3 }}>\n                        <Form onSubmit={userLogin}>\n                            <Input\n                                label=\"Email\"\n                                type=\"email\"\n                                placeholder=\"Email\"\n                                value={email}\n                                onChange={(e) => setEmail(e.target.value)}\n                            />\n\n                            <Input\n                                label=\"Password\"\n                                type=\"password\"\n                                placeholder=\"Password\"\n                                value={password}\n                                onChange={(e) => setPassword(e.target.value)}\n                            />\n                            <Button variant=\"primary\" type=\"submit\">\n                                Submit\n                            </Button>\n                        </Form>\n                    </Col>\n                </Row>\n            </Container>\n        </Layout>\n    )\n}\n\nexport default Signin\n","/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/containers/SignUp/index.js",["137","138"],"/Users/trancongtung/Documents/ReactJS/ĐATN-mern-ecommerce/admin-app/src/components/Layout/index.js",["139"],"/Users/trancongtu